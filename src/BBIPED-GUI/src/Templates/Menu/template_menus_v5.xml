					<help>
						<definition> TO BE COMPLETED  </definition>
						<values>(NO, YES)</values>
						<otherdetails>  TO BE COMPLETED </otherdetails>
					</help>
					<comboValues>
						<comboValue>NO</comboValue>
						<comboValue>YES</comboValue>
					</comboValues>
					<value name="RESTART_SOL" defaultValue="NO">NO</value>
				</variable>
